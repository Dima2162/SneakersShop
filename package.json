{
	"name": "sneakersshop",
	"version": "0.1.0",
	"private": true,
	"homepage": "https://Dima2162.github.io/SneakersShop",
	"dependencies": {
		"@testing-library/jest-dom": "^5.16.5",
		"@testing-library/react": "^13.4.0",
		"@testing-library/user-event": "^13.5.0",
		"macro-css": "^1.0.5",
		"react": "^18.2.0",
		"react-dom": "^18.2.0",
		"react-scripts": "5.0.1",
		"sass": "^1.59.2",
		"web-vitals": "^2.1.4"
	},
	"scripts": {
		"predeploy": "npm run build",
    "deploy": "gh-pages -d build",
		"dev": "concurrently \"npm run server\" \"npm start\"",
		"start": "set PORT=3005 && react-scripts start",
		"build": "react-scripts build",
		"test": "react-scripts test",
		"eject": "react-scripts eject",
		"server": "json-server -w server/sneakers.json -p 3002"
	},
	"eslintConfig": {
		"extends": [
			"react-app",
			"react-app/jest"
		]
	},
	"browserslist": {
		"production": [
			">0.2%",
			"not dead",
			"not op_mini all"
		],
		"development": [
			"last 1 chrome version",
			"last 1 firefox version",
			"last 1 safari version"
		]
	},
	"devDependencies": {
		"concurrently": "^7.6.0",
		"gh-pages": "^5.0.0",
		"json-server": "^0.17.2"
	}
}
